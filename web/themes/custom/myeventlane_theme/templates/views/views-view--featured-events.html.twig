{#
/**
 * @file
 * Theme override for the Featured Events view.
 * Outputs with proper wrapper structure for grid CSS.
 */
#}
<div class="view view-featured-events">
  <div class="view-content">
    {{ rows }}
  </div>
  {% if pager %}
    {{ pager }}
  {% endif %}
  {% if empty %}
    <div class="view-empty">
      <p>No featured events at this time.</p>
    </div>
  {% endif %}
</div>





