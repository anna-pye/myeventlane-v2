{#
/**
 * @file
 * Theme override for the Featured Events view.
 * Wraps rows in canonical event grid for consistent layout.
 */
#}
{% if rows %}
  <div{{ attributes.addClass('mel-event-grid') }}>
    {{ rows }}
  </div>
{% endif %}
{% if pager %}
  {{ pager }}
{% endif %}
{% if empty %}
  <div class="view-empty">
    <p>No featured events at this time.</p>
  </div>
{% endif %}





