{#
/**
 * @file
 * Theme override for unformatted views rows - Featured Events.
 * Removes wrapper divs to let CSS grid work properly.
 */
#}
{% if rows %}
  {% if title %}
    <h3>{{ title }}</h3>
  {% endif %}
  <div class="mel-event-grid">
    {% for row in rows %}
      {{ row.content }}
    {% endfor %}
  </div>
{% endif %}





