{#
/**
 * @file
 * Template override for Featured Events unformatted rows.
 * Uses .mel-featured-hero wrapper for single hero row layout (max 1-2 items).
 */
#}

{% if rows %}
  <div class="mel-featured-hero">
    {% for row in rows %}
      {{- row.content -}}
    {% endfor %}
  </div>
{% endif %}
